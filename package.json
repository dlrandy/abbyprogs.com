{
  "name": "abbyprogs.com",
  "version": "0.1.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint": "eslint --fix --ext .ts,.tsx ",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prepare": "husky install",
    "test": "jest --maxWorkers=50% --passWithNoTests",
    "test:watch": "jest --watch --maxWorkers=25%",
    "test:ci": "jest --runInBand",
    "release": "standard-version",
    "major": "yarn release --release-as major",
    "minor": "yarn release --release-as minor",
    "patch": "yarn release --release-as patch"
  },
  "dependencies": {
    "@chakra-ui/react": "1.6.5",
    "@emotion/react": "11.4.1",
    "@emotion/styled": "11.3.0",
    "@testing-library/react": "12.0.0",
    "framer-motion": "4.1.17",
    "jest": "27.0.6",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "ts-jest": "27.0.4",
    "twin.macro": "2.6.2"
  },
  "devDependencies": {
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@emotion/jest": "11.3.0",
    "@testing-library/cypress": "^8.0.0",
    "@types/jest": "26.0.24",
    "@types/node": "16.4.11",
    "@types/react": "17.0.15",
    "@types/react-dom": "17.0.9",
    "@typescript-eslint/eslint-plugin": "4.29.0",
    "@typescript-eslint/parser": "4.29.0",
    "cypress": "^8.2.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-jest": "24.4.0",
    "eslint-plugin-prettier": "3.4.0",
    "husky": "7.0.1",
    "lint-staged": "11.1.1",
    "prettier": "2.3.2",
    "standard-version": "^9.3.1",
    "typescript": "4.3.5",
    "vite": "2.4.4",
    "vite-plugin-babel-macros": "1.0.5",
    "vite-plugin-svgr": "0.3.0",
    "vite-plugin-windicss": "1.2.7",
    "vite-preset-react": "1.3.0",
    "windicss": "3.1.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dlrandy/abbyprogs.com.git"
  },
  "babelMacros": {
    "twin": {
      "preset": "emotion"
    }
  },
  "publishConfig": {
    "access": "restricted"
  },
  "license": "MIT"
}
